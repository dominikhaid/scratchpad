.modal .content {
color: black;
}

.ui.small.basic.modal .content{
  color:@white;
}

.ui.modal.active > div {
  background: @darkGray;
}

.ui.modal > div.header {
  background: @primaryColor;
  color: @white;
}

.ui.modal.active input {
  color: @primaryColor !important;
}

.ui.modal.active input::placeholder {
  color: @darkGray !important;
}